<?xml version="1.0"?>
<web-app>
	<display-name>MhyFlix</display-name>
	<description>MhyFlix local streaming of videos</description>
	<servlet>
		<servlet-name>FlixLogin</servlet-name>
		<servlet-class>FlixUser</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FlixVideos</servlet-name>
		<servlet-class>FlixVideos</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FlixLogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FlixVideos</servlet-name>
		<url-pattern>/videos-list</url-pattern>
	</servlet-mapping>
</web-app>
